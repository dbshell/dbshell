<?xml version="1.0" encoding="utf-8"?>
<Batch xmlns="http://schemas.dbshell.com/core" xmlns:rds="http://schemas.dbshell.com/datasync">
    <rds:SyncModel >
        <rds:SyncModel.Sources>
            <rds:Source DataSource="{Table 'dbo.Source'}" >
                <rds:SourceColumn Name="ID" IsKey="True" DataType="int" />
                <rds:SourceColumn Name="Value1" DataType="int" />
            </rds:Source>
        </rds:SyncModel.Sources>
        <rds:SyncModel.Targets>
            <rds:Target TableSchema="dbo" TableName="Target">
                <rds:Target.Columns>
                    <rds:TargetColumn Name="IdOriginal" Source="ID" IsKey="True" />
                    <rds:TargetColumn Name="Value1" Source="Value1" />
                    <rds:TargetColumn Name="ValidFrom" SpecialValue="ImportDateTime" />
                    <rds:TargetColumn Name="ValidTo" SpecialValue="Null" IsRestriction="True" />
                </rds:Target.Columns>
                <rds:Target.AdditionalFilters />
                <rds:Target.LifetimeHandler>
                    <rds:SimpleLifetimeHandler Update="True" Insert="True" Delete="False" />
                </rds:Target.LifetimeHandler>
            </rds:Target>
        </rds:SyncModel.Targets>
        <rds:SyncModel.TargetReferences />
        <rds:SyncModel.LogHandlers />
        <rds:SyncModel.Parameters />
    </rds:SyncModel>
    <rds:Run />
</Batch>