<?xml version="1.0" encoding="utf-8"?>
<Batch xmlns="http://schemas.dbshell.com/core" xmlns:rds="http://schemas.dbshell.com/datasync">
    <rds:SyncModel >
        <rds:SyncModel.Sources>
            <rds:Source DataSource="{Table 'dbo.Source2'}" >
                <rds:SourceColumn Name="ID" IsKey="True" />
                <rds:SourceColumn Name="Value1" />
            </rds:Source>
        </rds:SyncModel.Sources>
        <rds:SyncModel.Targets>
            <rds:Target TableSchema="dbo" TableName="Target2">
                <rds:Target.Columns>
                    <rds:TargetColumn Name="IdOriginal" Source="ID" IsKey="True" CompareSelectorFunction="ABS"/>
                    <rds:TargetColumn Name="Value1" Source="Value1" />
                    <rds:TargetColumn Name="IsImported" Value="1" IsUpdateRestriction="True" />
                </rds:Target.Columns>
                <rds:Target.AdditionalFilters />
            </rds:Target>
        </rds:SyncModel.Targets>
    </rds:SyncModel>
    <rds:Run />
</Batch>