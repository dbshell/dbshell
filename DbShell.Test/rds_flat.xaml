<?xml version="1.0" encoding="utf-8" ?>
<Batch 
    xmlns="http://schemas.dbshell.com/core"
    xmlns:rds="http://schemas.dbshell.com/datasync"
    >
    <rds:SyncModel IsFlatSync='True'>
        <rds:SyncModel.Sources>
            <rds:Source DataSource="{Table Source1}">
                <rds:SourceColumn Name="Id" />
                <rds:SourceColumn Name="Value" />
            </rds:Source>
            <rds:Source DataSource="{Table Source2}">
                <rds:SourceColumn Name="Id" />
                <rds:SourceColumn Name="Value" />
            </rds:Source>
        </rds:SyncModel.Sources>
        <rds:SyncModel.Targets>
            <rds:Target TableName="Target1" PrimarySource="Source1">
                <rds:TargetColumn Name="Id" Source="Id" IsKey="True"/>
                <rds:TargetColumn Name="Value" Source="Value" />
            </rds:Target>
			
            <rds:Target TableName="Target2" PrimarySource="Source2">
                <rds:TargetColumn Name="Id" Source="Id" IsKey="True"/>
                <rds:TargetColumn Name="Value" Source="Value" />
            </rds:Target>
        </rds:SyncModel.Targets>
    </rds:SyncModel>
    
    <rds:CreateProcedure ProcName="RunSync1" />
    <rds:CreateProcedure ProcName="RunSync2" />

    <rds:Run />
    <rds:Run />
</Batch>
