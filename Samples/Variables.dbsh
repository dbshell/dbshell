  <SetVariable Name="ConnectionNoDb" Value="" />
  <!-- define database name -->
  <SetVariable Name="DbName" Value="Chinook" />
  
  <SetVariable Name="Connection" Value="${ConnectionNoDb};Initial Catalog=${DbName}" />
</Batch>
