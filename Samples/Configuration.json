{
    "$type": "batch",
    "commands": [
        {
            "$type": "setVariable",
            "name": "ConnectionNoDb",
            "value": "sqlserver://Data Source=localhost\\SQLEXPRESS;MultipleActiveResultSets=True;Integrated Security=SSPI"
        },
        {
            "$type": "setVariable",
            "name": "DbName",
            "value": "Chinook"
        },
        {
            "$type": "setVariable",
            "name": "Connection",
            "value": "${ConnectionNoDb};Initial Catalog=${DbName}"
        }
    ]
}
