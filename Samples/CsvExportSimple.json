{
    "$type": "batch",
    "commands": [
        {
            "$type": "include",
            "file": "Configuration.json"
        },
        {
            "$type": "copyTable",
            "source": {
                "$type": "table",
                "name": "album",
                "connection": "${Connection}"
            },
            "target": {
                "$type": "file",
                "name": "album.csv"
            }
        }
    ]
}
