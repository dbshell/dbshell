<?xml version="1.0" encoding="utf-8" ?>
<Batch 
    xmlns="http://schemas.dbshell.com/core"
    xmlns:rds="http://schemas.dbshell.com/datasync"
    >
    <rds:SyncModel >
        <rds:SyncModel.Sources>
            <rds:Source DataSource="{Table Source}">
                <rds:SourceColumn Name="SourceId" />
                <rds:SourceColumn Name="Value" />
            </rds:Source>
        </rds:SyncModel.Sources>
        <rds:SyncModel.Targets>
            <rds:Target TableName="Target">
                <rds:TargetColumn Name="TargetIdOriginal" Source="SourceId" IsKey="True"/>
                <rds:TargetColumn Name="Value" Source="Value" />
                <rds:TargetColumn Name="ParamId" Expression="@ResValue" IsRestriction="True" />
            </rds:Target>
        </rds:SyncModel.Targets>
        <rds:SyncModel.Parameters>
            <rds:Parameter Name="ResValue" DefaultValue="99" DataType="int" />
        </rds:SyncModel.Parameters>
    </rds:SyncModel>

    <rds:CreateProcedure ProcName="RunSync1" />

    <rds:Run />
    
    <rds:Run >
        <rds:Run.Parameters>
            <rds:ParameterValue Name="ResValue" Value="33" />
        </rds:Run.Parameters>
    </rds:Run>
</Batch>
